// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db?timezone=+07:00"
}

model adminUser {
  id       Int    @id @default(autoincrement())
  name     String @unique
  password String
}

model RPLDmember {
  id   Int      @id @default(autoincrement())
  name String
  nim  String
  lab  String
  time DateTime @default(now())
}

model RPLDalat {
  id   Int      @id @default(autoincrement())
  name String
  nim  String
  lab  String
  time DateTime @default(now())
  alat String
}

model RPLDdata {
  id   Int    @id @default(autoincrement())
  name String
  nim  String
  lab  String
}
